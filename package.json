{
	"name": "ecs-parallel",
	"version": "0.0.1",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "rm -rf dist && babel src -d dist",
		"start": "node src/index.js",
		"watch": "npm-watch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@material-ui/core": "^3.9.2",
		"npm-watch": "^0.6.0",
		"typeface-roboto": "0.0.54"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-proposal-object-rest-spread"
		]
	},
	"watch": {
		"start": {
			"patterns": [
				"src"
			],
			"extensions": "js",
			"quite": false
		}
	}
