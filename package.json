{
	"name": "ecs-parallel",
	"version": "0.0.4",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"build": "rm -rf dist && babel src -d dist",
		"start-test-app": "npm run build && node dist/test-app.js",
		"watch": "npm-watch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@babel/polyfill": "^7.4.3",
		"npm-watch": "^0.6.0"
	},
	"dependencies": {
		"parallel-job-runner": "0.0.8"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-proposal-object-rest-spread"
		]
	},
	"watch": {
		"start-test-app": {
			"patterns": [
				"src"
			],
			"extensions": "js",
			"quite": false
		}
	}
}
